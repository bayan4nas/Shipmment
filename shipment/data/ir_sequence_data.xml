<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <!-- Sequences for shipement.order -->
        <record id="seq_shipment_order" model="ir.sequence">
            <field name="name">Shipment Order</field>
            <field name="code">shipment.order</field>
            <field name="prefix">S</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
         <!-- Sequences for inv seq-->
         <record id="inv_seq" model="ir.sequence">
            <field name="name">Inv seq</field>
            <field name="code">account.move.seq</field>
            <field name="prefix">RSEA</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>
